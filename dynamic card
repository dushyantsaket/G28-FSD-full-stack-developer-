      function alldata() {
        let cardcontainer = document.getElementById("card-container");
        allData.forEach((item) => {
          let div = document.createElement("div");
          div.style.border = "1px solid black";
          div.style.width = "300px";
          div.style.padding = "10px";
          div.style.display = "flex";
          div.style.margin = "4px";
          div.style.flexDirection = "column";
          div.style.alignItems = "center";

          let cardcontainer = document.getElementById("card-container");
          cardcontainer.style.display = " grid";
          cardcontainer.style.gridTemplateColumns = " repeat(4,1fr)";
          cardcontainer.style.gap = " 10px ";
          cardcontainer.style.margin = " 10px ";

          let card = document.createElement("img");
          card.style.height = "150px";
          card.style.width = "100%";
          card.src = item.img;

          let title = document.createElement("h3");
          title.innerText = item.title;
          title.style.color = "black";
          title.style.textAlign = "center";

          let KeySpecifications = document.createElement("p")
          KeySpecifications.innerText= item.KeySpecifications;
          KeySpecifications.style.color=" black";
          KeySpecifications.style.textAlign="center";

          let decscription = document.createElement("p");
          decscription.innerText = item.decscription;
          decscription.style.color = "black";
          decscription.style.textAlign = "center";

          let btn = document.createElement("button");
          btn.innerText = "buy now";
          btn.style.backgroundColor = "black";
          btn.style.color = "white";

          div.append(card, title, decscription,KeySpecifications, btn);
          cardcontainer.appendChild(div);
        });
      }

      alldata();
</script>
    
